{
  "name": "svgo-config",
  "version": "0.4.0",
  "description": "A svgo config to keep svg files clean",
  "license": "MIT",
  "author": {
    "name": "Eric Chen",
    "email": "airkro@qq.com"
  },
  "keywords": [
    "config",
    "minify",
    "svg",
    "svgo"
  ],
  "homepage": "https://github.com/best-shot/svgo-config",
  "repository": {
    "type": "git",
    "url": "https://github.com/best-shot/svgo-config.git"
  },
  "bugs": {
    "url": "https://github.com/best-shot/svgo-config/issues"
  },
  "module": "lib/config.js",
  "main": "lib/config.cjs",
  "exports": {
    ".": "./lib/config.js",
    "./lib/*": "./lib/*",
    "./package.json": "./package.json"
  },
  "files": [
    "lib"
  ],
  "type": "module",
  "scripts": {
    "lint": "nice-move lint",
    "prepare": "git config core.hooksPath .githooks",
    "prepublishOnly": "npm run lint && npm test",
    "test": "ava --fail-fast"
  },
  "devDependencies": {
    "@nice-move/cli": "^0.6.0",
    "@nice-move/commitlint-config": "^0.4.0",
    "@nice-move/eslint-config-base": "^0.5.75",
    "@nice-move/prettier-config": "^0.5.1",
    "ava": "^3.15.0",
    "commitlint": "^15.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-ava": "^13.1.0",
    "garou": "^0.3.4",
    "prettier": "^2.5.1",
    "svgo": "^2.8.0"
  },
  "peerDependencies": {
    "svgo": "^2.8.0"
  },
  "engines": {
    "node": "^12.22.0 || ^14.17.0 || >=16.13.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "commitlint": {
    "extends": "@nice-move/commitlint-config"
  },
  "eslintConfig": {
    "extends": "@nice-move/eslint-config-base"
  },
  "prettier": "@nice-move/prettier-config",
  "x-readme": {
    "logo": "https://cdn.jsdelivr.net/gh/svg/svgo/logo/isotype.svg"
  }
}